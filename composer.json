{
    "type": "project",
    "license": "GPL-3.0-or-later",
    "name": "robiningelbrecht/statistics-for-strava",
    "description": "Statistics for Strava is a self-hosted web app designed to provide you with better stats.",
    "authors": [
        {
            "name": "Robin Ingelbrecht",
            "email": "ingelbrecht_robin@hotmail.com"
        }
    ],
    "keywords": [
        "PHP",
        "strava",
        "self-hosted"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "adrenalinkin/doctrine-naming-strategy": "^3.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.3",
        "guzzlehttp/guzzle": "*",
        "league/flysystem-bundle": "^3.3",
        "league/flysystem-memory": "^3.29",
        "moneyphp/money": "^4.5",
        "nesbot/carbon": "*",
        "ramsey/uuid": "^4.7",
        "respect/validation": "^2.3",
        "symfony/asset": "7.3.*",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/expression-language": "7.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.3.*",
        "symfony/intl": "7.3.*",
        "symfony/lock": "7.3.*",
        "symfony/messenger": "7.3.*",
        "symfony/monolog-bundle": "*",
        "symfony/process": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/translation": "7.3.*",
        "symfony/twig-bundle": "7.3.*",
        "symfony/yaml": "7.3.*",
        "twig/twig": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^12",
        "robiningelbrecht/phpunit-coverage-tools": "^1.8",
        "robiningelbrecht/phpunit-pretty-print": "^1.3",
        "spatie/phpunit-snapshot-assertions": "^5.1",
        "symfony/phpunit-bridge": "7.3.*",
        "symfony/stopwatch": "7.3.*",
        "symfony/web-profiler-bundle": "7.3.*",
        "tomasvotruba/type-coverage": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "@compress-intl"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "@compress-intl"
        ],
        "compress-intl": "php ./vendor/symfony/intl/Resources/bin/compress"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    }
}
